{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "strict": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "paths": {
            "$routes/*": [
                "./src/routes/*"
            ],
            "$lib/*": [
                "./src/lib/*"
            ],
            "$lib": [
                "./src/lib"
            ]
        }
    }
}
